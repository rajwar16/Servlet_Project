<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>EmployForm</display-name>
	<welcome-file-list>
		<welcome-file>EmployLoginForm.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
	<servlet-name>RegistrationForm</servlet-name>
	<servlet-class>com.bridgelabz.EmployForm.Model.RegistrationForm</servlet-class>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>RegistrationForm</servlet-name>
	<url-pattern>/RegistrationForm</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>LoginForm1</servlet-name>
	<servlet-class>com.bridgelabz.EmployForm.Model.LoginForm</servlet-class>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>LoginForm1</servlet-name>
	<url-pattern>/LoginForm</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>EmployeeDetailss</servlet-name>
	<servlet-class>com.bridgelabz.EmployForm.Model.EmployeeDetails</servlet-class>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>EmployeeDetailss</servlet-name>
	<url-pattern>/EmployeeDetails</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
	<servlet-name>EmployeeLogout1</servlet-name>
	<servlet-class>com.bridgelabz.EmployForm.Model.EmployeeLogout</servlet-class>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>EmployeeLogout1</servlet-name>
	<url-pattern>/EmployeeLogout</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>UserRegistration</servlet-name>
	<servlet-class>com.bridgelabz.EmployForm.Model.UserRegistration</servlet-class>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>UserRegistration</servlet-name>
	<url-pattern>/UserRegistration</url-pattern>
	</servlet-mapping>
</web-app>